<?xml version="1.0" encoding="utf-8"?>
<object>
	<title>Rectangle</title>
	<svg width="50" height="50">
		<defs>
			<filter id="shadowFilter">
				<feGaussianBlur stdDeviation="3" />
			</filter>
		</defs>
		<use id="shadow" xlink:href="#rect" x="2" y="2" style="visibility:hidden;fill:#404040;filter:url(#shadowFilter)" />
		<g id="main" fill="blue" fill-opacity="1.0">
			<rect id="rect" x="0%" y="0%" width="100%" height="100%" rx="5" ry="5"/>
		</g>
	</svg>
	<parameters>
		<parameter>
			<title>Rx</title>
			<name>rx</name>
			<type>integer</type>
			<element>rect</element>
			<attribute>rx</attribute>
		</parameter>
		<parameter>
			<title>Ry</title>
			<name>ry</name>
			<type>integer</type>
			<element>rect</element>
			<attribute>ry</attribute>
		</parameter>
		<parameter>
			<title>Colour</title>
			<name>fill</name>
			<type>colour</type>
			<element>main</element>
			<attribute>fill</attribute>
		</parameter>
		<parameter>
			<title>Shadow</title>
			<name>shadow</name>
			<type>shadow</type>
			<element>shadow</element>
			<attribute>fill</attribute>
		</parameter>
	</parameters>
	<min-size>
		<width>8</width>
		<height>8</height>
	</min-size>
</object>
